{
	"name": "bingo-web",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"postinstall": "prisma generate",
		"dev": "next dev --turbo",
		"build": "prisma generate && next build --turbo",
		"start": "next start",
		"lint:check": "biome lint .",
		"lint:fix": "biome lint --write .",
		"format:check": "biome format .",
		"format:fix": "biome format --write .",
		"spell:check": "cspell \"**/*\"",
		"knip:check": "knip",
		"type:check": "echo \"No type check (Next.js doesn't recommend tsc: https://github.com/vercel/next.js/issues/53732)\"",
		"validate:check": "pnpm format:check && pnpm lint:check && pnpm type:check && pnpm spell:check && pnpm knip:check",
		"validate:fix": "pnpm format:fix && pnpm lint:fix",
		"test:unit": "vitest",
		"db:deploy": "prisma migrate deploy"
	},
	"packageManager": "pnpm@10.19.0",
	"prisma": {
		"seed": "pnpm dlx tsx prisma/seed.ts"
	},
	"dependencies": {
		"@harusame0616/result": "0.1.2",
		"@next/third-parties": "15.5.6",
		"@prisma/client": "6.15.0",
		"@radix-ui/react-alert-dialog": "1.0.4",
		"@radix-ui/react-dialog": "1.0.4",
		"@radix-ui/react-icons": "1.3.2",
		"@radix-ui/react-label": "2.1.7",
		"@radix-ui/react-radio-group": "1.3.8",
		"@radix-ui/react-slot": "1.0.2",
		"@radix-ui/react-toast": "1.2.15",
		"autoprefixer": "10.4.21",
		"class-variance-authority": "0.7.1",
		"clsx": "2.1.1",
		"jotai": "2.15.0",
		"next": "15.5.6",
		"postcss": "8.5.6",
		"react": "19.1.1",
		"react-dom": "19.1.1",
		"tailwind-merge": "1.14.0",
		"tailwindcss": "3.4.18",
		"tailwindcss-animate": "1.0.7",
		"valibot": "1.1.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.7",
		"@playwright/test": "1.38.0",
		"@types/node": "24.3.3",
		"@types/react": "19.1.13",
		"@types/react-dom": "19.1.9",
		"@vitejs/plugin-react": "4.7.0",
		"cspell": "9.2.1",
		"jsdom": "22.1.0",
		"knip": "5.66.0",
		"prisma": "6.15.0",
		"tsx": "4.20.5",
		"typescript": "5.9.3",
		"vitest": "0.34.3"
	}
}
